//3.5 Task 5: Countdown
//Print out numbers from 10 to 0 using a loop.
process.stdout.write(" 10 seconds to ignition... Starting countdown... \n")
let i = Number(10);
    while  (i >= 1){
        process.stdout.write("..."+ i +"...\n")
            i--;
    }
    process.stdout.write(" IGNITION!! ");
process.exit()
