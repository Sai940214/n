/** 
 * LOOP
 * For
 */

// FOR 可以有循环分支
// FOR nest loop

// Create a loop to create rows (0-9)
for ( let i = 0 ; i <= 9; i++){
    // Create a loop to create rows (0-9)
    for (let j = 0 ; j<= 9 ; j++)
        // Print out (i.j)
        process.stdout.write("(" + i + "." + j + ")");

//Print out a new line
    process.stdout.write("\n")
}
